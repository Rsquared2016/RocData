variable : M1 {
type: discrete hidden cardinality NUM_INDIVIDUALS;
switchingparents: nil;
conditionalparents: H2(0), H3(0) using DeterministicCPT("meeting_1");
  }

variable : M2 {
type: discrete hidden cardinality NUM_INDIVIDUALS;
switchingparents: nil;
conditionalparents: H1(0), H3(0) using DeterministicCPT("meeting_2");
  }

variable : M3 {
type: discrete hidden cardinality NUM_INDIVIDUALS;
switchingparents: nil;
conditionalparents: H1(0), H2(0) using DeterministicCPT("meeting_3");
  }

